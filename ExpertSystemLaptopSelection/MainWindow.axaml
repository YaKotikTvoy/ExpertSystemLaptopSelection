<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="ExpertSystemLaptopSelection.MainWindow"
        Title="ExpertSystemLaptopSelection">
    
    <TabControl Margin="5" Name="MainTabControl">
        <!-- БЗО - Без заданного определения-->
        <TabItem Header="Экспертная система выбора ноутбука" Name="Start">
            <StackPanel>
            <Label HorizontalAlignment="Center" VerticalAlignment="Center">Выполнил студент ИСб-4</Label>
            <Label HorizontalAlignment="Center" VerticalAlignment="Center">Сединин Артём</Label>
            <Button HorizontalAlignment="Center" VerticalAlignment="Center" 
                    Click="StartButtonClick"
                    Name="StartButton">Начать выбирать ноутбук</Button>
            </StackPanel>
        </TabItem>



        <!-- 1 - Goal-->
        <TabItem Header="Для чего ноутбук?" Name="Goal">
                <Grid ColumnDefinitions="Auto,Auto"
                    RowDefinitions="Auto,Auto,Auto,Auto">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="70*"/>
                        <ColumnDefinition Width="30*"/>
                    </Grid.ColumnDefinitions>
                    
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <RadioButton Name="Goal2" GroupName="Goal" Content="Играть"
                        Grid.Row="1" Grid.Column="0"/>
                    <RadioButton Name="Goal35" GroupName="Goal" Content="Учеба"
                        Grid.Row="2" Grid.Column="0"/>
                    <RadioButton Name="Goal41" GroupName="Goal" Content="Работать"
                        Grid.Row="3" Grid.Column="0"/>
                    <Button Name="BackGreeting" Click="BackGreetingButtonClick" Grid.Row="4" Grid.Column="0">На страницу приветствия</Button>    
                    <Button Click="" Grid.Row="4" Grid.Column="1">Далее</Button>

                    <!--<Label Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Top" Background="Aquamarine">Предыдущие вопросы</Label>-->
                    <!--<Label Grid.Column="1" Grid.Row="1" Grid.RowSpan="3" HorizontalAlignment="Center" VerticalAlignment="Top">1.Для чего необходим ноутбук?</Label>-->
                </Grid>
            <!--<StackPanel>
                <Label >Для чего необходим ноутбук?</Label>
                <RadioButton Name="Goal2" GroupName="Goal" Content="Играть"/>
                <RadioButton Name="Goal35" GroupName="Goal" Content="Учеба"/>
                <RadioButton Name="Goal41" GroupName="Goal" Content="Работать"/>
                <Button Click="GoalButtonClicked">Далее</Button>
            </StackPanel>-->
        </TabItem>















        <!-- 2 - Dimension-->
        <TabItem Header="Какая n-мерность ваших игр?" Name="Dimension">
            <!--<DockPanel LastChildFill="True">-->
                <Grid ColumnDefinitions="Auto,Auto"
                    RowDefinitions="Auto,Auto,Auto,Auto">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="70*"/>
                        <ColumnDefinition Width="30*"/>
                    </Grid.ColumnDefinitions>
                    
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <RadioButton Name="Dimension3" GroupName="Dimension" Content="2D, 2.5D"
                        Grid.Row="1" Grid.Column="0"/>
                    <RadioButton Name="Dimension12" GroupName="Dimension" Content="3D"
                        Grid.Row="2" Grid.Column="0"/>
                    <Button Name="BackGoal" Click="BackGoalButtonClicked" Grid.Row="4" Grid.Column="0">К предыдущему вопросу</Button>    
                    <Button Click="" Grid.Row="4" Grid.Column="1">Далее</Button>

                    <Label Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Top" Background="Aquamarine">Предыдущие вопросы</Label>
                    <Label Grid.Column="1" Grid.Row="1" Grid.RowSpan="3" HorizontalAlignment="Center" VerticalAlignment="Top">1.Для чего необходим ноутбук?</Label>
                </Grid>
            <!--</DockPanel>-->
        </TabItem>


















        <!-- 3 - CountPlayer2-->
        <TabItem Header="Количество игроков?" Name="CountPlayer2">
            <!--<DockPanel LastChildFill="True">-->
                <Grid ColumnDefinitions="Auto,Auto"
                    RowDefinitions="Auto,Auto,Auto,Auto">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="70*"/>
                        <ColumnDefinition Width="30*"/>
                    </Grid.ColumnDefinitions>
                    
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <RadioButton Name="CountPlayer2_4" GroupName="CountPlayer2" Content="Многопользовательская/сетевая"
                        Grid.Row="1" Grid.Column="0"/>
                    <RadioButton Name="CountPlayer2_5" GroupName="CountPlayer2" Content="Одиночная"
                        Grid.Row="2" Grid.Column="0"/>
                    <Button Name="BackDimension" Click="BackDimensionButtonClick" Grid.Row="4" Grid.Column="0">На страницу приветствия</Button>    
                    <Button Click="" Grid.Row="4" Grid.Column="1">Далее</Button>

                    <Label Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Top" Background="Aquamarine">Предыдущие вопросы</Label>
                    <Label Grid.Column="1" Grid.Row="1" Grid.RowSpan="3" HorizontalAlignment="Center" VerticalAlignment="Top">
                        1.Для чего необходим ноутбук?
                        2.Какая n-мерность ваших игр?
                        </Label>
                </Grid>
            <!--</DockPanel>-->
        </TabItem>




















